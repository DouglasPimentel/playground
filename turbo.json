{
  "$schema": "https://turbo.build/schema.json",
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**", ".next/**"]
    },
    "lint": {
      "outputs": []
    },
    "dev": {
      "cache": false,
      "env": ["NODE_ENV", "API_PORT", "JWT_SECRET", "MONGO_URI"]
    },
    "start": {
      "outputs": []
    },
    "format": {
      "outputs": []
    }
  }
}
